<?xml version="1.0" encoding="utf-8"?>
<window>
    <defaultcontrol always="true">5</defaultcontrol>
    <controls>
        <include>DimBG</include>
        <control type="group">
            <include>DialogOpenClose</include>
            <control type="image">
                <posx>160</posx>
                <posy>140</posy>
                <width>1600</width>
                <height>1080</height>
                <!--<include>DialogTransparency</include>-->
                <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
                <texture border="50">common/background_menu.png</texture>
            </control>
            <include name="WindowClosingButton">
                <param name="id" value="9010"/>
                <param name="x" value="1710"/>
                <param name="y" value="140"/>
                <param name="onleft" value="6060"/>
            </include>
            <control type="label" id="2">
                <posx>260</posx>
                <posy>240</posy>
                <width>1400</width>
                <height>50</height>
                <font>InfoTitleFont</font>
                <include>TextColor_MainColor</include>
                <label>-</label>
                <scroll>true</scroll>
            </control>
            <control type="label">
                <posx>260</posx>
                <posy>282</posy>
                <width>1400</width>
                <height>35</height>
                <font>InfoTitleSubFont</font>
                <include>TextColor_MidColor</include>
                <label>$INFO[Container(2).NumItems,, $LOCALIZE[31025]]</label>
            </control>
            <control type="list" id="5">
                <description>Preset List</description>
                <posx>210</posx>
                <posy>340</posy>
                <width>1500</width>
                <height>600</height>
                <itemgap>0</itemgap>
                <orientation>vertical</orientation>
                <onleft>noop</onleft>
                <onright>60</onright>
                <pagecontrol>60</pagecontrol>
                <onup>noop</onup>
                <ondown>noop</ondown>
                <itemlayout width="1500" height="50">
                    <control type="label">
                        <posx>TextMargin</posx>
                        <height>50</height>
                        <width>1400</width>
                        <font>FacadeListMainFont</font>
                        <textcolor>NoFocusedColor</textcolor>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                </itemlayout>
                <focusedlayout width="1500" height="50">
                    <control type="group">
                        <include>ButtonWideFocusAnimation</include>
                        <control type="image">
                            <posx>0</posx>
                            <posy>0</posy>
                            <height>50</height>
                            <width>1500</width>
                            <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
                            <texture border="1">list/bg_facade_focus.png</texture>
                        </control>
                        <control type="label">
                            <posx>TextMargin</posx>
                            <height>50</height>
                            <width>1400</width>
                            <font>FacadeListMainFont</font>
                            <include>TextColor_MainColor</include>
                            <scroll>true</scroll>
                            <label>$INFO[ListItem.Label]</label>
                        </control>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="60">
                <posx>1660</posx>
                <posy>380</posy>
                <height>600</height>
                <include>VerticalScroller</include>
                <onleft>5</onleft>
                <onright>60</onright>
            </control>
        </control>
    </controls>
</window>

<?xml version="1.0" encoding="utf-8"?>
<includes>
    <include name="SkinShortcuts_Window_Back_Level">
        <control type="multiimage">
            <visible>!Skin.HasSetting(DisableFanart)</visible>
            <posx>530</posx>
            <posy>330</posy>
            <width>1340</width>
            <height>700</height>
            <imagepath background="true">$VAR[SkinShortcutBackgrounds]</imagepath>
            <aspectratio aligny="top">scale</aspectratio>
            <timeperimage>5000</timeperimage>
            <fadetime>400</fadetime>
            <randomize>true</randomize>
        </control>
        <control type="image">
            <posx>530</posx>
            <posy>330</posy>
            <width>1340</width>
            <height>350</height>
            <colordiffuse>$VAR[OverlayTransparency]</colordiffuse>
            <texture>$VAR[Theme.Color]/common/overlay_top.png</texture>
        </control>
        <control type="image">
            <posx>530</posx>
            <posy>680</posy>
            <width>1340</width>
            <height>350</height>
            <colordiffuse>$VAR[OverlayTransparency]</colordiffuse>
            <texture>$VAR[Theme.Color]/common/overlay_bottom.png</texture>
        </control>
        <control type="image">
            <posx>530</posx>
            <posy>520</posy>
            <width>1340</width>
            <height>510</height>
            <texture border="55">common/background_strip_horizontal.png</texture>
        </control>
        <control type="image">
            <posx>530</posx>
            <posy>766</posy>
            <width>1340</width>
            <height>268</height>
            <texture border="65">common/background_strip_horizontal_text.png</texture>
        </control>
    </include>
    <include name="SkinShortcuts_Window_Navigation">
        <control type="group">
            <control type="button" id="3010">
                <posx>530</posx>
                <posy>620</posy>
                <width>536</width>
                <height>151</height>
                <texturefocus>-</texturefocus>
                <texturenofocus>-</texturenofocus>
                <onclick>Control.Move(211,-1)</onclick>
            </control>
            <control type="button" id="3020">
                <posx>1334</posx>
                <posy>620</posy>
                <width>536</width>
                <height>151</height>
                <texturefocus>-</texturefocus>
                <texturenofocus>-</texturenofocus>
                <onclick>Control.Move(211,1)</onclick>
            </control>
        </control>
    </include>
    <include name="SkinShortcuts_MainMenu_Horizontal">
        <posx>530</posx>
        <posy>620</posy>
        <width>1340</width>
        <height>151</height>
        <itemgap>0</itemgap>
        <onright>211</onright>
        <onleft>211</onleft>
        <ondown>260</ondown>
        <onup>9000</onup>
        <orientation>horizontal</orientation>
        <viewtype label="535">list</viewtype>
        <preloaditems>2</preloaditems>
        <pagecontrol>260</pagecontrol>
        <movement>0</movement>
        <focusposition>2</focusposition>
        <itemlayout height="151" width="268">
            <control type="image">
                <visible>StringCompare(ListItem.Property(hide),True)+SubString(ListItem.Property(LabelID),weather,Left)</visible>
                <posx>0</posx>
                <posy>0</posy>
                <width>268</width>
                <height>151</height>
                <texture background="true">special://skin/backgrounds/Weather.jpg</texture>
                <aspectratio>scale</aspectratio>
            </control>
            <control type="image">
                <visible>StringCompare(ListItem.Property(hide),True)+!SubString(ListItem.Property(LabelID),weather,Left)</visible>
                <posx>0</posx>
                <posy>0</posy>
                <width>268</width>
                <height>151</height>
                <texture background="true" fallback="special://skin/backgrounds/default/default.jpg">$INFO[ListItem.Icon]</texture>
                <aspectratio>scale</aspectratio>
            </control>
            <control type="image">
                <visible>!StringCompare(ListItem.Property(hide),True)</visible>
                <posx>0</posx>
                <posy>0</posy>
                <width>268</width>
                <height>151</height>
                <colordiffuse>90ffffff</colordiffuse>
                <texture background="true" fallback="special://skin/backgrounds/default/default.jpg">$INFO[ListItem.Icon]</texture>
                <aspectratio>scale</aspectratio>
            </control>
        </itemlayout>
        <focusedlayout height="151" width="268">
            <control type="image">
                <visible>StringCompare(ListItem.Property(hide),True)+SubString(ListItem.Property(LabelID),weather,Left)</visible>
                <posx>0</posx>
                <posy>0</posy>
                <width>268</width>
                <height>151</height>
                <texture background="true">special://skin/backgrounds/Weather.jpg</texture>
                <aspectratio>scale</aspectratio>
            </control>
            <control type="image">
                <visible>StringCompare(ListItem.Property(hide),True)+!SubString(ListItem.Property(LabelID),weather,Left)</visible>
                <posx>0</posx>
                <posy>0</posy>
                <width>268</width>
                <height>151</height>
                <texture background="true" fallback="special://skin/backgrounds/default/default.jpg">$INFO[ListItem.Icon]</texture>
                <aspectratio>scale</aspectratio>
            </control>
            <control type="image">
                <visible>!StringCompare(ListItem.Property(hide),True)</visible>
                <posx>0</posx>
                <posy>0</posy>
                <width>268</width>
                <height>151</height>
                <colordiffuse>90ffffff</colordiffuse>
                <texture background="true" fallback="special://skin/backgrounds/default/default.jpg">$INFO[ListItem.Icon]</texture>
                <aspectratio>scale</aspectratio>
            </control>
            <control type="image">
                <posx>0</posx>
                <posy>0</posy>
                <width>268</width>
                <height>151</height>
                <texture border="1,50,1,50">$VAR[Theme.Color]/common/frame_focus.png</texture>
                <aspectratio>stretch</aspectratio>
            </control>
        </focusedlayout>
        <include condition="Skin.HasSetting(plexbmc)+Window.isactive(CustomPlexHomeSettings.xml)">Plexbmc.Items.SkinSettings</include>
    </include>
</includes>
